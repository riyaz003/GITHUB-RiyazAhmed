<!DOCTYPE html>
<html>
<head>
    <title>Test 11</title>
    <script>
        function manipulateArray(array) {
            // Add a new string "VOLVO" to the end of the array
            array.push("VOLVO");

            // Iterate over each element and convert to uppercase
            return array.map(element => element.toUpperCase());
        }

        function displayResult() {
            let inputArray = ["bmw", "audi", "mercedes"];
            let result = manipulateArray(inputArray);
            document.getElementById("result").textContent = result.join(", ");
        }
    </script>
</head>
<body>
    <h1>Manipulate Array</h1>
    <button onclick="displayResult()">Manipulate Array</button>
    <p id="result"></p>
</body>
</html>
