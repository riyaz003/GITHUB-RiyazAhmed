<script>

    
let myString = "Hello, world!";      
let myNumber = 42;                    

let myBoolean = true;           
let myUndefined;                    
let myNull = null;                    



console.log(myString, "Type is:", typeof myString);      
console.log(myNumber,"Type is:", typeof myNumber);     

console.log( myBoolean,"Type is:", typeof myBoolean);     
console.log(myUndefined,"Type is:", typeof myUndefined); 
console.log(myNull,"Type is:", typeof myNull);           
      

    
</script>

